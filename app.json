{
  "expo": {
    "name": "Focus Time",
    "description": "Time to focus on one specific task or event that you care !",
    "slug": "focusTime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/timer.png",
    "splash": {
      "image": "./assets/FocusTime.png",
      "resizeMode": "contain",
      "backgroundColor": "#3c6382"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/timer.png",
        "backgroundColor": "#3c6382"
      }
    },
    "web": {
      "favicon": "./assets/timer.png"
    }
  }
}
